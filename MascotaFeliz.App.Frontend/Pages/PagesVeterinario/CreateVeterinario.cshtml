@page
@model MascotaFeliz.App.Frontend.Pages.CreateVeterinarioModel
@{
    ViewData["Title"] = "Home page";
    Layout=null;
    

}

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>RegistroMascota </title>
    <link rel="stylesheet" href="/css/Style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com/%22%3E">
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Silkscreen&display=swap" rel="stylesheet">

</head>
<body>
    <a href="/Index" ><img src="/Imagenes/atras.png" id="atras"></a>
    <div class="caja1">
        <h1 id="titulo">Registro</h1>
        <img src="/Imagenes/perrogato.png" id="perrogato">





    </div>
    <form class="caja2" method="post">
        <input type="hidden" asp-for="veterinario.Id">
        <input class="inputtxt" asp-for="veterinario.Nombres"   placeholder="Ingrese Nombre ">
        <input class="inputtxt" asp-for="veterinario.Apellidos"  placeholder="Ingrese el Apellido ">
        <input class="inputtxt"  asp-for="veterinario.Direccion" placeholder="Ingrese la Direccion ">
        <input class="inputtxt"  asp-for="veterinario.Telefono"  placeholder="Ingrese el telefono ">
        <input class="inputtxt"  asp-for="veterinario.TarjetaProfesional" placeholder="Ingrese Tarjeta Profecional ">
        
        

        <button class="boton" id="btnRegistrar" type="submit">Registrar</button>

        <a href="/Front/Consulta.html" id="ya">¿Ya estas registrado? </a>

        </form>

        
       
        




    
    

    
</body>


