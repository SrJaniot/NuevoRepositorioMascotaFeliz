    <link href="https://fonts.googleapis.com/css2?family=Silkscreen&display=swap" rel="stylesheet"


    
  <body>


    <header>
        <div class="Encabezado_superior">
            
            <div class="Logo">
              
                <img id="Logo" src="~/Imagenes/perrogato.png" alt="">

                <br>
                <h1 class="titulo">Bienvenidos</h1>
                <h1 class="titulo">Mascota Feliz</h1>
                <a href="/Burger/Creditos"><img  id="logopb" src="~/Imagenes/Logo pb.png" alt=""  ></a>
               

            </div>


        </div>


        <div class="Encabezado_Barramenu">
            <div class="menu">
                <nav>
                    <ul>
                        <li  ><a href="/Index">Inicio</a> </li>
                        <li><a href="">Registrar</a>
                            <ul>
                                <li><a href="/PagesMascota/CreateMascota">Registrar Mascota</a></li>
                                <li><a href="/PagesDueno/CreateDueno">Registrar Dueño</a></li>
                                <li><a href="/PagesVeterinario/CreateVeterinario">Registrar Veterinario</a></li>
                            </ul>
                        </li>
                        <li><a href="">Consultar</a>
                            <ul>
                                <li><a href="/PagesMascota/BuscarMascota">Buscar Mascota</a></li>
                                <li><a href="/PagesMascota/ListaMascotas">Mostrar Mascotas</a></li>
                                <li><a href="/PagesDueno/ListaDuenos">Mostrar Duenos</a></li>
