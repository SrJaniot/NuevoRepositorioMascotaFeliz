                    </ul>
                </nav>


            </div>


        </div>
    </header>
    <h1 id="listadodemascotas">Listado de Dueños</h1>






 
    <div class="cajat">
        <table class="tabla">

            <tr>
          
              <th>ID</th>
          
              <th>nombre</th>
          
              <th>Apellido</th>

              <th>Telefono</th>

              <th>Dirección</th>

              

              <th>Correo</th>
              <th>update</th>

          
            </tr>

            
            @foreach (var x in Model.ListaDuenos)
            {
                <tr>
                    <td>@x.Id</td>
                    <td>@x.Nombres</td>
                    <td>@x.Apellidos</td>
                    <td>@x.Telefono</td>
                    <td>@x.Direccion</td>
                    <td>@x.Correo</td>
                    
                    
                    <td ><a asp-page ="/PagesDueno/CreateDueno" id="Estado" asp-route-dddduenoId="@x.Id" >update</a></td>


                </tr>
                
            }
          
            
            
          
             
            
            
            
          
      
            
           
            
            
              
          
          </table>
    </div>

   

    </body>


